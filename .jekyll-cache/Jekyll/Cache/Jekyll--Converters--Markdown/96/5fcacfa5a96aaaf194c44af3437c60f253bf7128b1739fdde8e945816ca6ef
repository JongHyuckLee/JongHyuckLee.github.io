    <span class="k">import</span> <span class="p">{</span><span class="nx">createEventDispatcher</span><span class="p">}</span> <span class="k">from</span> <span class="dl">'</span><span class="s1">svelte</span><span class="dl">'</span>
    <span class="kd">const</span> <span class="nx">dispatch</span> <span class="o">=</span> <span class="nx">createEventDispatcher</span><span class="p">();</span>
    <span class="k">export</span> <span class="kd">let</span> <span class="nx">onAlert</span>

    <span class="kd">function</span> <span class="nx">innerOnAlert</span> <span class="p">()</span> <span class="p">{</span>
        <span class="nx">dispatch</span><span class="p">(</span><span class="dl">'</span><span class="s1">onAlert2</span><span class="dl">'</span><span class="p">,</span> <span class="p">{</span>
            <span class="na">text</span><span class="p">:</span> <span class="dl">'</span><span class="s1">Svelte2</span><span class="dl">'</span><span class="p">,</span>
            <span class="na">value</span><span class="p">:</span> <span class="dl">'</span><span class="s1">Svelte Emit</span><span class="dl">'</span>
        <span class="p">})</span>
    <span class="p">}</span>
